{
    "name": "yiisoft/file",
    "type": "library",
    "description": "file",
    "keywords": [
        "file"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/file/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/file"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "cycle/annotated": "^2.0",
        "cycle/orm": "^1.2",
        "league/flysystem": "1.0.63",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-azure-blob-storage": "^0.1.6",
        "league/flysystem-cached-adapter": "^1.0",
        "league/flysystem-memory": "^1.0",
        "league/flysystem-rackspace": "^1.0",
        "league/flysystem-replicate-adapter": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "league/flysystem-webdav": "^1.0",
        "league/flysystem-ziparchive": "^1.0",
        "royvoetman/flysystem-gitlab-storage": "^1.0",
        "spatie/flysystem-dropbox": "^1.2",
        "superbalist/flysystem-google-storage": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "phan/phan": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\file\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\File\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "scripts": {
        "phan": "phan --progress-bar -o analysis.txt"
    },
    "config": {
        "sort-packages": true
    }
}
